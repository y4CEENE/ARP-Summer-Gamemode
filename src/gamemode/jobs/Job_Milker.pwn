hook OnLoadPlayer(playerid, row)
{
    PlayerData[playerid][pBocketFull] = GetDBIntField(row, "bocketfull");
	PlayerData[playerid][pBocket] = GetDBIntField(row, "bocket");
}

// Milker

static Milker_Payment = 7802;
new LastBucketPurchase[MAX_PLAYERS];

CMD:buybocket(playerid, params[])
{
    if (!PlayerHasJob(playerid, JOB_MILKER))
    {
        return SendClientMessage(playerid, COLOR_GREY, "You can't use this command as you are not a Milker.");
    }
    if (!IsPlayerInRangeOfPoint(playerid, 5, 1034.360595, -281.772613, 73.998100))
    {
        return SendClientMessage(playerid, -1, "You are not near any bucket table.");
    }
    if (PlayerData[playerid][pCash] < 1500)
    {
        return SendClientMessage(playerid, COLOR_DARKGREEN, "You need at least $1,500 to buy a bucket.");
    }

    if (gettime() - LastBucketPurchase[playerid] < 150) // 180 seconds = 3 minutes
    {
        return SendClientMessage(playerid, COLOR_GREY, "You can only buy a bucket every 2.5 minutes.");
    }

    // Update the last purchase time
    LastBucketPurchase[playerid] = gettime();

    GivePlayerCash(playerid, -1500);
    PlayerData[playerid][pBocket] += 1;

    DBQuery("UPDATE "#TABLE_USERS" SET bocket = %i WHERE uid = %i", 
                 PlayerData[playerid][pBocket], PlayerData[playerid][pID]);

    SendClientMessage(playerid, COLOR_OLDSCHOOL, "MILKER: You have purchased a bucket for $1,500.");
    return 1;
}

new LastMilkTime[MAX_PLAYERS];

CMD:milk(playerid, params[])
{
    if (!PlayerHasJob(playerid, JOB_MILKER))
    {
        return SendClientMessage(playerid, COLOR_GREY, "You can't use this command as you are not a Milker.");
    }
    if (PlayerData[playerid][pBocket] <= 0)
    {
        return SendClientMessage(playerid, COLOR_GREY, "You don't have a bucket.");
    }
    if (!IsPlayerInRangeOfPoint(playerid, 3.0, 1067.486083, -304.230407, 74.008110) &&
        !IsPlayerInRangeOfPoint(playerid, 3.0, 1066.052612, -299.234252, 74.008110) &&
        !IsPlayerInRangeOfPoint(playerid, 3.0, 1063.673095, -295.457275, 74.008110) &&
        !IsPlayerInRangeOfPoint(playerid, 3.0, 1071.917480, -295.437622, 74.008110) &&
        !IsPlayerInRangeOfPoint(playerid, 3.0, 1075.167724, -302.154174, 74.008110) &&
        !IsPlayerInRangeOfPoint(playerid, 3.0, 1079.390014, -309.615386, 74.008110) &&
        !IsPlayerInRangeOfPoint(playerid, 3.0, 1081.445556, -304.313812, 74.008110) &&
        !IsPlayerInRangeOfPoint(playerid, 3.0, 1071.877929, -308.532379, 74.008110) &&
        !IsPlayerInRangeOfPoint(playerid, 3.0, 1068.574829, -313.870330, 74.008110) &&
        !IsPlayerInRangeOfPoint(playerid, 3.0, 1062.901489, -308.476959, 74.008110))
    {
        return SendClientMessage(playerid, 0xFF0000FF, "You are not near a cow!");
    }

    if (gettime() - LastMilkTime[playerid] < 120) // 120 seconds = 2 minutes
    {
        return SendClientMessage(playerid, COLOR_GREY, "You can only milk a cow every 2 minutes.");
    }

    // Update the last milking time
    LastMilkTime[playerid] = gettime();

    // Milking animation & object attachment
    SetPlayerAttachedObject(playerid, 1, 19468, 6, 0.308999, 0.020000, 0.000000, 15.600001, -103.199974, -2.500001, 1.000000, 1.000000, 1.000000);
    ApplyAnimation(playerid, "ROB_BANK", "CAT_Safe_Rob", 4.0, 1, 0, 0, 0, 0, 1); // Milking animation

    SendClientMessage(playerid, 0x00FF00FF, "You are milking the cow...");

    // Deduct an empty bucket and increase the full bucket count
    PlayerData[playerid][pBocket] -= 1;
    SetTimerEx("GiveMilk", 6500, false, "d", playerid);
    PlayerData[playerid][pBocketFull] += 1;

    return 1;
}

// Timer function to give milk after delay
forward GiveMilk(playerid);
public GiveMilk(playerid)
{
    ClearAnimations(playerid); // Stop the milking animation
    SendClientMessage(playerid, 0x00FF00FF, "You have milked the cow and got a fullbucket of milk!");
    return 1;
}

new LastMilkSale[MAX_PLAYERS];

CMD:sellmilk(playerid, params[])
{
    if (!IsPlayerInRangeOfPoint(playerid, 5, 1082.199340, -365.259033, 74.074180))
    {
        return SendClientMessage(playerid, COLOR_GREY, "You are not near the Milkman.");
    }

    if (gettime() - LastMilkSale[playerid] < 120) // 120 seconds = 2 minutes
    {
        return SendClientMessage(playerid, COLOR_GREY, "You can only sell milk every 2 minutes.");
    }

    if (PlayerData[playerid][pBocketFull] > 0)
    {
        PlayerData[playerid][pBocketFull] -= 1;

        // Update the last sale time
        LastMilkSale[playerid] = gettime();

        DBQuery("UPDATE "#TABLE_USERS" SET bocketfull = %i WHERE uid = %i",
                     PlayerData[playerid][pBocketFull], PlayerData[playerid][pID]);

        ApplyAnimation(playerid, "CARRY", "crry_prtial", 4.0, 0, 0, 0, 0, 0, 1);
        RemovePlayerAttachedObject(playerid, 1);
        RemovePlayerAttachedObject(playerid, 9);
        GivePlayerCash(playerid, Milker_Payment);

        SendClientMessage(playerid, 0x00FF00FF, "You sold a bucket of milk and earned money!");
    }
    else
    {
        SendClientMessage(playerid, 0xFF0000FF, "You have no milk to sell!");
    }

    return 1;
}